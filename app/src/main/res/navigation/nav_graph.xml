<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/authorizationFragment">
    <fragment
        android:id="@+id/authorizationFragment"
        android:name="com.example.kormopack.authorizationFragment.AuthorizationFragment"
        android:label="fragment_authorization"
        tools:layout="@layout/fragment_authorazation" >
        <action
            android:id="@+id/action_authorizationFragment_to_nav_specs"
            app:destination="@id/nav_specs"
            app:popUpTo="@id/authorizationFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/nav_specs"
        android:name="com.example.kormopack.navFragments.SpecsFragment"
        android:label="@string/kormo_specs"
        tools:layout="@layout/fragment_specs">
        <action
            android:id="@+id/action_nav_specs_to_authorizationFragment"
            app:destination="@id/authorizationFragment"
            app:popUpTo="@id/nav_specs"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_specs_to_nav_cabinet"
            app:destination="@id/nav_cabinet"
            app:popUpTo="@id/nav_specs"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_specs_to_nav_settings"
            app:destination="@id/nav_settings"
            app:popUpTo="@id/nav_specs"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_specs_to_nav_about"
            app:destination="@id/nav_about"
            app:popUpTo="@id/nav_specs"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_specs_to_nav_pay_calc"
            app:destination="@id/nav_pay_calc"
            app:popUpTo="@id/nav_specs"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_specs_to_feedListFragment"
            app:destination="@id/feedListFragment" />
        <action
            android:id="@+id/action_nav_specs_to_nav_instruction"
            app:destination="@id/nav_instruction"
            app:popUpTo="@id/nav_specs"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/nav_cabinet"
        android:name="com.example.kormopack.navFragments.PersonalCabinetFragment"
        android:label="@string/kormo_cabinet"
        tools:layout="@layout/fragment_personal_cabinet" >
        <action
            android:id="@+id/action_nav_cabinet_to_nav_specs"
            app:destination="@id/nav_specs"
            app:popUpTo="@id/nav_cabinet"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_cabinet_to_authorizationFragment"
            app:destination="@id/authorizationFragment"
            app:popUpTo="@id/nav_cabinet"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_cabinet_to_nav_settings"
            app:destination="@id/nav_settings"
            app:popUpTo="@id/nav_cabinet"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_cabinet_to_nav_about"
            app:destination="@id/nav_about"
            app:popUpTo="@id/nav_cabinet"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_cabinet_to_nav_pay_calc"
            app:destination="@id/nav_pay_calc"
            app:popUpTo="@id/nav_cabinet"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_cabinet_to_nav_instruction"
            app:destination="@id/nav_instruction"
            app:popUpTo="@id/nav_cabinet"
            app:popUpToInclusive="true"/>
        <argument
            android:name="user"
            app:argType="com.google.android.gms.auth.api.signin.GoogleSignInAccount" />

    </fragment>
    <fragment
        android:id="@+id/nav_settings"
        android:name="com.example.kormopack.navFragments.SettingsFragment"
        android:label="@string/kormo_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_nav_settings_to_nav_cabinet"
            app:destination="@id/nav_cabinet"
            app:popUpTo="@id/nav_settings"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_settings_to_nav_specs"
            app:destination="@id/nav_specs"
            app:popUpTo="@id/nav_settings"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_settings_to_authorizationFragment"
            app:destination="@id/authorizationFragment"
            app:popUpTo="@id/nav_settings"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_settings_to_nav_about"
            app:destination="@id/nav_about"
            app:popUpTo="@id/nav_settings"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_settings_to_nav_pay_calc"
            app:destination="@id/nav_pay_calc"
            app:popUpTo="@id/nav_settings"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_settings_to_nav_instruction"
            app:destination="@id/nav_instruction"
            app:popUpTo="@id/nav_settings"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/nav_about"
        android:name="com.example.kormopack.navFragments.AboutFragment"
        android:label="@string/kormo_about"
        tools:layout="@layout/fragment_about" >
        <action
            android:id="@+id/action_nav_about_to_nav_settings"
            app:destination="@id/nav_settings"
            app:popUpTo="@id/nav_about"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_about_to_nav_cabinet"
            app:destination="@id/nav_cabinet"
            app:popUpTo="@id/nav_about"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_about_to_nav_specs"
            app:destination="@id/nav_specs"
            app:popUpTo="@id/nav_about"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_about_to_authorizationFragment"
            app:destination="@id/authorizationFragment"
            app:popUpTo="@id/nav_about"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_about_to_nav_pay_calc"
            app:destination="@id/nav_pay_calc"
            app:popUpTo="@id/nav_about"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_about_to_nav_instruction"
            app:destination="@id/nav_instruction"
            app:popUpTo="@id/nav_about"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/feedListFragment"
        android:name="com.example.kormopack.brandRecycleClasses.FeedListFragment"
        tools:layout="@layout/fragment_feed_list" >
        <action
            android:id="@+id/action_feedListFragment_to_nav_specs"
            app:destination="@id/nav_specs" />
    </fragment>
    <fragment
        android:id="@+id/nav_pay_calc"
        android:name="com.example.kormopack.navFragments.PayCalcFragment"
        android:label=""
        tools:layout="@layout/fragment_pay_calc" >
        <action
            android:id="@+id/action_nav_pay_calc_to_nav_specs"
            app:destination="@id/nav_specs"
            app:popUpTo="@id/nav_pay_calc"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_pay_calc_to_nav_about"
            app:destination="@id/nav_about"
            app:popUpTo="@id/nav_pay_calc"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_pay_calc_to_nav_cabinet"
            app:destination="@id/nav_cabinet"
            app:popUpTo="@id/nav_pay_calc"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_pay_calc_to_nav_settings"
            app:destination="@id/nav_settings"
            app:popUpTo="@id/nav_pay_calc"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_pay_calc_to_nav_instruction"
            app:destination="@id/nav_instruction"
            app:popUpTo="@id/nav_pay_calc"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/nav_instruction"
        android:name="com.example.kormopack.navFragments.InstructionFragment"
        android:label="@string/kormo_instruction"
        tools:layout="@layout/fragment_instruction" >
        <action
            android:id="@+id/action_nav_instruction_to_nav_settings"
            app:destination="@id/nav_settings"
            app:popUpTo="@id/nav_instruction"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_instruction_to_nav_cabinet"
            app:destination="@id/nav_cabinet"
            app:popUpTo="@id/nav_instruction"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_instruction_to_nav_pay_calc"
            app:destination="@id/nav_pay_calc"
            app:popUpTo="@id/nav_instruction"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_instruction_to_nav_about"
            app:destination="@id/nav_about"
            app:popUpTo="@id/nav_instruction"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_instruction_to_nav_specs"
            app:destination="@id/nav_specs"
            app:popUpTo="@id/nav_instruction"
            app:popUpToInclusive="true"/>
    </fragment>
</navigation>